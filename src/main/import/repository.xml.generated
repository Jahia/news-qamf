<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <news j:dependencies="default grid seo"
            j:installedModules="default"
            j:modulePriority="0"
            j:moduleType="module"
            j:title="Jahia News"
            jcr:primaryType="jnt:module">
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder"/>
                    <contents jcr:primaryType="jnt:contentFolder"/>
                    <news-template j:applyOn="jnt:newsQamf"
                           j:defaultTemplate="true"
                           j:priority="100"
                           jcr:primaryType="jnt:contentTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <row column="1col16" jcr:primaryType="jnt:row"/>
                        </pagecontent>
                        <row-col1 jcr:primaryType="jnt:contentList">
                            <mainResourceDisplay j:mainResourceView="fullpage" jcr:primaryType="jnt:mainResourceDisplay"/>
                        </row-col1>
                    </news-template>
                    <news-template-link j:applyOn="jnt:newsQamf"
                                j:defaultTemplate="false"
                                j:priority="100"
                                jcr:primaryType="jnt:contentTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <row column="1col16" jcr:primaryType="jnt:row"/>
                        </pagecontent>
                        <row-col1 jcr:primaryType="jnt:contentList">
                            <mainResourceDisplay j:mainResourceView="link" jcr:primaryType="jnt:mainResourceDisplay"/>
                        </row-col1>
                    </news-template-link>
                </templates>
                
            </news>
    </modules>
</content>
